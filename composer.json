{
   "name": "giuseppe998e/syringe",
   "description": "Dependency injection library for PHP8",
   "version": "1.0.3",
   "type": "library",
   "license": "Apache-2.0",
   "homepage": "https://github.com/giuseppe998e/syringe",
   "authors": [
      {
         "name": "Giuseppe Eletto",
         "email": "giuseppe@eletto.me",
         "homepage": "https://giuseppe.eletto.me"
      }
   ],
   "autoload": {
      "psr-4": {
         "Syringe\\": "src/Syringe/"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "Syringe\\" : "tests/Syringe/"
      }
   },
   "require": {
      "php": "^8"
   },
   "require-dev": {
      "phpunit/phpunit": "^9.5"
   }
}
